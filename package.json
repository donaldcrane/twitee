{
    "name": "twitee",
    "version": "1.0.0",
    "description": "A mini and substandard runoff of Twitter",
    "main": "app.js",
    "scripts": {
        "clean": "npm rm -rf build && npm run build-babel",
        "build-babel": "babel -d ./build ./src -s",
        "build": "npm run clean && npm run build-babel",
        "start": "npm run build && node ./build/app.js",
        "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
        "test": "cross-env NODE_ENV=test && npm run migration && npm run seeder && nyc --require --reporter=html --reporter=text mocha -r @babel/register ./src/tests/index.js --timeout 100000 --recursive --exit || true ",
        "coverage": "nyc npm run test && nyc report --reporter=text-lcov --reporter=lcov | node ./node_modules/coveralls/bin/coveralls.js --verbose",
        "lint": "eslint src/  --fix",
        "migration": "npm run migrate:undo && npm run migrate",
        "migrate": "node_modules/.bin/sequelize db:migrate",
        "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
        "seeder": "npm run seed:undo && npm run seed",
        "seed": "node_modules/.bin/sequelize db:seed:all",
        "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all"
    },
    "repository": {
        "type": "git",
        "url": "git@bitbucket.org:retailspot/rs-spotowner-be.git"
    },
    "keywords": [],
    "author": "Donald Agbakoba",
    "license": "ISC",
    "homepage": "https://github.com/donaldcrane/twitee",
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.13.16",
        "@babel/node": "^7.13.13",
        "@babel/plugin-transform-runtime": "^7.13.15",
        "@babel/preset-env": "^7.13.15",
        "@babel/register": "^7.13.16",
        "@babel/runtime": "^7.13.17",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "cookie-session": "^1.4.0",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.25.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^32.3.3",
        "mocha": "^8.3.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "sequelize-test-helpers": "^1.3.3",
        "sinon": "^10.0.0",
        "sinon-chai": "^3.6.0"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.5.0",
        "bcrypt": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "joi": "^17.2.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "path": "^0.12.7",
        "pg": "^8.6.0",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.6.2",
        "sequelize-cli": "^6.2.0",
        "pino": "^6.2.1",
        "pino-pretty": "^7.0.0",
        "validator": "^13.6.0"
    }
}
